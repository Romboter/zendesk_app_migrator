    "mem-fs-editor": "^3.0.2",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "node-emoji": "^1.8.1",
    "progress": "^2.0.0",
    "recast": "^0.12.6",
    "shelljs": "^0.7.8",
    "tslib": "^1.7.1"
  },
